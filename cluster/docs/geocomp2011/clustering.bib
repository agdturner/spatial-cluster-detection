  keywords = {cluster, health, spatial},
  owner = {ijt1},
  posted-at = {2008-12-13 19:29:32},
  priority = {3},
  timestamp = {2011.01.20},
  url = {http://dx.doi.org/10.1186/1476-072X-7-61}
}

