  day = {27},
  doi = {10.1186/1471-2105-9-247},
  issn = {1471-2105},
  keywords = {clustering, infovis, statistical, visualization},
  posted-at = {2008-08-04 15:02:51},
  priority = {3},
  url = {http://dx.doi.org/10.1186/1471-2105-9-247}
}

